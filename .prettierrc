{
	"tabWidth": 2,
	"semi": false,
	"useTabs": true,
	"singleQuote": false,
	"arrowParens": "always",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/controllers/(.*)$",
		"^@/routers/(.*)$",
		"^@/util/(.*)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
